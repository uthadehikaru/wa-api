# WhatsApp API Configuration
# Copy this file to .env and fill in your values

# API Security Token (REQUIRED)
# This token must be included in all API requests as Authorization header
# Format: Bearer <your-token-here>
API_TOKEN=your-secure-api-token-here

# Server Configuration
PORT=3000
NODE_ENV=development

# WhatsApp Configuration
# Add any other WhatsApp-specific environment variables here
# For example:
# WHATSAPP_SESSION_NAME=my-session
# WHATSAPP_DATA_PATH=./auth

# Logging Configuration
LOG_LEVEL=debug
